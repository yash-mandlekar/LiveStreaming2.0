<div class="remoteStreams" ng-controller="RemoteStreamsController as rtc">
  <p class="watch-live">लाइव देखें</p>
  <div id="remoteVideosContainer"></div>

  <table>
    <thead></thead>
    <tbody>
      <tr
        ng-repeat="stream in rtc.remoteStreams"
        ng-class="{ selected: stream.isPlaying }"
      >
        <td>
          <a class="three">{{ stream.name }}</a>
        </td>
        <td ng-click="rtc.view(stream)"><a class="three">View</a></td>
        <td ng-click="rtc.call(stream)"><a class="three">Call</a></td>
      </tr>
    </tbody>
  </table>

  <li id="refreshcnt" ng-click="rtc.loadData()">
    <a id="refresh" href>Refresh</a>
  </li>
  <div class="remoteStreams"></div>
</div>
