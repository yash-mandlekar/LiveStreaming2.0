<!DOCTYPE html>
<html ng-app="projectRtc">
  <head>
    <title>View page</title>
    <base href="/" />
    <link rel="stylesheet" href="/stylesheets/style.css" />

    <script type="text/javascript" src="/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="javascripts/adapter.js"></script>
    <script type="text/javascript" src="javascripts/rtcClient.js"></script>

    <script
      type="text/javascript"
      src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"
    ></script>
    <script type="text/javascript" src="javascripts/app.js"></script>
  </head>

  <body>
    <div class="container"><%= include remote-streams %></div>
  </body>
  <script>
    var table = document.querySelector("table");
    var midcontainer = document.querySelector(".midcontainer");
    table.addEventListener("click", function (e) {
      console.log("classname --- >  ", e.target.className);
      if (
        e.target.className === "viewimg" ||
        e.target.className === "three ng-binding"
      ) {
        midcontainer.style.display = "none";
      }
    });
  </script>
</html>
